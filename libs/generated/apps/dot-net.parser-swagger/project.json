{
    "name": "apps/dot-net.parser-swagger",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "targets": {
        "build": {
            "executor": "nx:noop",
            "outputs": ["libs/generated/apps/dot-net.parser-swagger"]
        },
        "codegen": {
            "executor": "@nx-dotnet/core:openapi-codegen",
            "options": {
                "useOpenApiGenerator": true,
                "openApiGenerator": "typescript",
                "openapiJsonPath": "libs/generated/apps/dot-net.parser-swagger",
                "outputProject": "generated-apps/dot-net.parser-types"
            },
            "dependsOn": ["swagger"],
            "inputs": [
                "{projectRoot}/libs/generated/apps/dot-net.parser-swagger"
            ],
            "outputs": ["{workspaceRoot}"]
        }
    },
    "implicitDependencies": ["apps/dotNet.Parser"]
}
